{
  "name": "pacman-js",
  "type": "module",
  "author": "Brent Ward",
  "nyc": {
    "include": [
      "app/scripts/**/*.js"
    ],
    "all": true
  },
  "scripts": {
    "test": "nyc --reporter=html --reporter=text --reporter=json --check-coverage --lines 100 --functions 100 --branches 100 mocha app/tests",
    "posttest": "node scripts/coverage-summary.js",
    "lint": "eslint .",
    "update": "ncu -i --format group",
    "serve": "node ./node_modules/http-server/bin/http-server --cors",
    "watch": "gulp watch",
    "prepare": "husky"
    
  },
  "engines": {
    "node": ">=12.18.3",
    "npm": ">=6.14.6"
  },
  "devDependencies": {
    "eslint": "^4.1.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.32.0",
    "gulp": "^5.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-remove-code": "^3.0.4",
    "gulp-sass": "^6.0.1",
    "http-server": "^14.1.1",
    "husky": "^9.1.7",
    "mocha": "^11.3.0",
    "npm-check-updates": "^19.3.1",
    "nyc": "^17.1.0",
    "sass": "^1.97.3",
    "sinon": "^21.0.1"
  },
  "dependencies": {
    "cors": "^2.8.6",
    "express": "^5.2.1"
  }
}
